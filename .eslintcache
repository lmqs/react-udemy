tém as tarefas\r\n            const tarefasDb = localStorage['tarefas'];\r\n            const tarefas = tarefasDb ? JSON.parse(tarefasDb) : [];\r\n\r\n            //persistir a tarefa\r\n            tarefas.push(new Tarefa(new Date().getTime(), tarefa, false));\r\n            localStorage['tarefas'] = JSON.stringify(tarefas);\r\n            setExibeModal(true);\r\n        }   \r\n    }\r\n\r\n    function handleTxtTarefa(event){\r\n        setTarefa(event.target.value);\r\n    }\r\n\r\n    function handleFecharModal(event){\r\n        setExibeModal(false);\r\n        <A href=\"/\"/>\r\n    }\r\n    return (\r\n        <div>\r\n            <h3 className=\"text-center\"> Cadastrar</h3>\r\n            <Jumbotron>\r\n                <Form validated={formValidado} noValidate onSubmit={cadastrar}>\r\n                    <Form.Group>\r\n                        <FormLabel>Tarefa</FormLabel>\r\n                        <Form.Control type=\"text\" placeholder=\"Digite a tarefa\"\r\n                            minLength=\"5\" maxLength=\"100\" required \r\n                            value={tarefa} \r\n                            onChange={handleTxtTarefa}\r\n                            data-testid=\"txt-tarefa\" //serve para a classe de teste\r\n                        />\r\n                    <Form.Control.Feedback type=\"invalid\">\r\n                        A tarefa deve conter ao menos 5 caracteres\r\n                    </Form.Control.Feedback>\r\n                    </Form.Group>\r\n                    <Form.Group className=\"text-center\">\r\n                        <Button variant=\"success\" type=\"submit\" data-testid=\"btn-cadastrar\">Cadastrar </Button>\r\n                        &nbsp;\r\n                        <A href=\"/\" className=\"btn btn-light\"> Voltar</A>\r\n                    </Form.Group>\r\n                </Form>\r\n                <Modal show={exibeModal} onHide={handleFecharModal} data-testid=\"modal\">\r\n                    <Modal.Header closeButton>\r\n                        <Modal.Title>Sucesso</Modal.Title>\r\n                    </Modal.Header>\r\n                    <Modal.Body>\r\n                        Tarefa adicionada com sucesso!\r\n                    </Modal.Body>\r\n                    <Modal.Footer>\r\n                        <Button variant=\"success\" \r\n                        onClick={handleFecharModal}  >Continuar</Button>\r\n                    </Modal.Footer>\r\n                </Modal>\r\n            </Jumbotron>\r\n        </div>\r\n    );\r\n}\r\n\r\n\r\nexport default CadastrarTarefa;","E:\\Estudo\\react\\udemy8projetos\\react-udemy\\src\\atualizar\\atualizar-tarefa.js",[],"E:\\Estudo\\react\\udemy8projetos\\react-udemy\\src\\listar\\itens-lista-tarefas.js",[],"E:\\Estudo\\react\\udemy8projetos\\react-udemy\\src\\models\\tarefa.model.js",[],"E:\\Estudo\\react\\udemy8projetos\\react-udemy\\src\\listar\\concluir-tarefa.js",[],"E:\\Estudo\\react\\udemy8projetos\\react-udemy\\src\\listar\\remover-tarefa.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":3,"column":9,"nodeType":"53","messageId":"54","endLine":3,"endColumn":17},"no-native-reassign",["55"],"no-negated-in-lhs",["56"],"no-unused-vars","'navigate' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]